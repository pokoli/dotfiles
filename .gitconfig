[user]
	name = Sergi Almacellas Abellana
	email = sergi@koolpi.com
[color]
	ui = auto
[core]
	autocrlf = input
	safecrlf = true
	excludesfile = /home/pokoli/.gitignoreglobal
[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  type = cat-file -t
  dump = cat-file -p
[pull]
	rebase = true
[push]
	default = matching
